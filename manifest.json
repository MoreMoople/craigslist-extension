// manifest.json
{
  "manifest_version": 2,
  "name": "My Navbar Extension",
  "version": "1.0",
  "description": "Add a navbar to webpages.",
  "permissions": ["activeTab"],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}

